{
  "$schema": "node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "4.3.1",
    "generators": {
      "mollys-movies-api": {
        "generatorName": "typescript-angular",
        "output": "src/app/api",
        "glob": "../mollys-movies.json",
        "additionalProperties": {
          "npmName": "restClient",
          "supportsES6": "true",
          "ngVersion": "13.3.9",
          "fileNaming": "kebab-case",
          "stringEnums": true,
          "useSingleRequestParameter": true
        }
      },
      "mollys-movies-api-stubs": {
        "generatorName": "typescript-axios",
        "output": "stubs/api",
        "glob": "../mollys-movies.json",
        "additionalProperties": {
          "supportsES6": "true",
          "fileNaming": "kebab-case",
          "stringEnums": true,
          "withSeparateModelsAndApi": true,
          "withNodeImports": true,
          "apiPackage": "api",
          "modelPackage": "model"

        }
      }
    }
  }
}
